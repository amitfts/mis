{
    "name": "efusionsoft/mis",
    "description": "Admin package for laravel 4",
    "keywords": ["laravel","admin","sentry", "authentication", "user", "group", "permission"],
    "license": "MIT",
    "authors": [
        {
            "name": "efusionsoft",
            "email": "amit.garg@efusionsoft.com"
        }
    ],
    "require":
    {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "cartalyst/sentry": "~2"
    },
    "require-dev": {
        "illuminate/database": "4.1.*",
        "mockery/mockery": "0.9.0",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "classmap": [
            "src/commands",
            "src/controllers",
            "src/helpers",
            "src/migrations",
            "src/models",
            "src/services"
        ],
        "psr-0": {
            "Efusionsoft\\Mis": "src/"
        }
    },

    "minimum-stability": "dev"
}
